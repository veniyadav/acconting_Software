generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model companies {
  id         BigInt         @id @default(autoincrement())
  name       String?        @db.VarChar(255)
  email      String?        @db.VarChar(255)
  status     CompanyStatus?
  created_at DateTime       @default(now()) @db.Timestamp(0)
}

model users {
  id            BigInt        @id @default(autoincrement())
  company_id    BigInt?
  name          String?       @db.VarChar(255)
  email         String?       @unique(map: "email") @db.VarChar(255)
  phone         String?       @db.VarChar(20)
  role          UserRole?
  status        users_status?
  password      String?       @db.VarChar(255)
  profile_image String?       @db.VarChar(255)
  created_at    DateTime      @default(now()) @db.Timestamp(0)

  @@index([company_id], map: "company_id")
}

model groups {
  id          BigInt   @id @default(autoincrement())
  company_id  BigInt?
  group_name  String?  @db.VarChar(100)
  description String?  @db.Text
  created_at  DateTime @default(now()) @db.Timestamp(0)

  @@index([company_id], map: "company_id")
}

model subgroups {
  id            BigInt   @id @default(autoincrement())
  group_id      BigInt?
  subgroup_name String?  @db.VarChar(100)
  description   String?  @db.Text
  created_at    DateTime @default(now()) @db.Timestamp(0)

  @@index([group_id], map: "group_id")
}

model accounts {
  id               BigInt                 @id @default(autoincrement())
  parent_id        BigInt?
  main_category    accounts_main_category
  subgroup         String                 @db.VarChar(100)
  account_name     String                 @db.VarChar(255)
  account_number   String?                @db.VarChar(50)
  ifsc_code        String?                @db.VarChar(20)
  bank_name_branch String?                @db.VarChar(255)
  is_active        Boolean?               @default(true)
  created_by       BigInt?
  updated_by       BigInt?
  created_at       DateTime               @default(now()) @db.Timestamp(0)
  updated_at       DateTime               @default(now()) @db.Timestamp(0)

  @@index([created_by], map: "created_by")
  @@index([parent_id], map: "parent_id")
  @@index([updated_by], map: "updated_by")
}

model financial_years {
  id         BigInt      @id @default(autoincrement())
  company_id BigInt?
  start_date DateTime?   @db.Date
  end_date   DateTime?   @db.Date
  status     YearStatus?

  @@index([company_id], map: "company_id")
}

model vouchers {
  id               BigInt                 @id @default(autoincrement())
  company_id       BigInt?
  voucher_type     vouchers_voucher_type?
  company_name     String?                @db.VarChar(255)
  company_logo     String?                @db.VarChar(255)
  from_customer_id BigInt?
  to_name          String?                @db.VarChar(255)
  receipt_number   String?                @db.VarChar(50)
  voucher_number   String?                @db.VarChar(50)
  voucher_date     DateTime?              @db.Date
  subtotal         Decimal?               @db.Decimal(15, 2)
  total            Decimal?               @db.Decimal(15, 2)
  notes            String?                @db.Text
  created_by       BigInt?
  created_at       DateTime               @default(now()) @db.Timestamp(0)

  @@index([company_id], map: "company_id")
  @@index([created_by], map: "created_by")
  @@index([from_customer_id], map: "from_customer_id")
}

model voucher_entries {
  id         BigInt   @id @default(autoincrement())
  voucher_id BigInt?
  account_id BigInt?
  debit      Decimal? @db.Decimal(15, 2)
  credit     Decimal? @db.Decimal(15, 2)
  narration  String?  @db.Text

  @@index([voucher_id], map: "voucher_id")
}

model transactions {
  id               BigInt    @id @default(autoincrement())
  voucher_entry_id BigInt?
  account_id       BigInt?
  txn_date         DateTime? @db.Date
  debit            Decimal?  @db.Decimal(15, 2)
  credit           Decimal?  @db.Decimal(15, 2)
  created_at       DateTime  @default(now()) @db.Timestamp(0)

  @@index([account_id], map: "account_id")
  @@index([voucher_entry_id], map: "voucher_entry_id")
}

model taxes {
  id          BigInt   @id @default(autoincrement())
  company_id  BigInt?
  tax_name    String?  @db.VarChar(100)
  tax_type    TaxType?
  rate        Decimal? @db.Decimal(5, 2)
  description String?  @db.Text
  created_at  DateTime @default(now()) @db.Timestamp(0)

  @@index([company_id], map: "company_id")
}

model discounts {
  id               BigInt        @id @default(autoincrement())
  voucher_entry_id BigInt?
  discount_type    DiscountType?
  amount           Decimal?      @db.Decimal(15, 2)
  created_at       DateTime      @default(now()) @db.Timestamp(0)

  @@index([voucher_entry_id], map: "voucher_entry_id")
}

model bills {
  id                 BigInt      @id @default(autoincrement())
  vendor_id          BigInt?
  voucher_id         BigInt?
  total_amount       Decimal?    @db.Decimal(15, 2)
  outstanding_amount Decimal?    @db.Decimal(15, 2)
  due_date           DateTime?   @db.Date
  status             BillStatus?
  created_at         DateTime    @default(now()) @db.Timestamp(0)

  @@index([vendor_id], map: "vendor_id")
  @@index([voucher_id], map: "voucher_id")
}

model bill_settlements {
  id                 BigInt   @id @default(autoincrement())
  bill_id            BigInt?
  payment_voucher_id BigInt?
  amount_paid        Decimal? @db.Decimal(15, 2)
  discount_applied   Decimal? @db.Decimal(15, 2)
  tds_deducted       Decimal? @db.Decimal(15, 2)
  created_at         DateTime @default(now()) @db.Timestamp(0)

  @@index([bill_id], map: "bill_id")
  @@index([payment_voucher_id], map: "payment_voucher_id")
}

model items {
  id            BigInt   @id @default(autoincrement())
  company_id    BigInt?
  item_name     String?  @db.VarChar(255)
  sku           String?  @db.VarChar(100)
  hsn_code      String?  @db.VarChar(50)
  unit          String?  @db.VarChar(50)
  opening_stock Decimal? @db.Decimal(15, 2)
  rate          Decimal? @db.Decimal(15, 2)
  created_at    DateTime @default(now()) @db.Timestamp(0)

  @@index([company_id], map: "company_id")
}

model voucher_items {
  id           BigInt   @id @default(autoincrement())
  voucher_id   BigInt?
  item_id      BigInt?
  qty          Decimal? @db.Decimal(15, 3)
  rate         Decimal? @db.Decimal(15, 2)
  discount     Decimal? @db.Decimal(15, 2)
  tax_id       BigInt?
  tax_amount   Decimal? @db.Decimal(15, 2)
  total_amount Decimal? @db.Decimal(15, 2)

  @@index([item_id], map: "item_id")
  @@index([tax_id], map: "tax_id")
  @@index([voucher_id], map: "voucher_id")
}

model attachments {
  id           BigInt   @id @default(autoincrement())
  voucher_id   BigInt?
  quotation_id BigInt?
  order_id     BigInt?
  return_id    BigInt?
  file_url     String?  @db.VarChar(255)
  file_type    String?  @db.VarChar(50)
  uploaded_by  BigInt?
  created_at   DateTime @default(now()) @db.Timestamp(0)

  @@index([uploaded_by], map: "uploaded_by")
}

model audit_logs {
  id         BigInt       @id @default(autoincrement())
  user_id    BigInt?
  table_name String?      @db.VarChar(100)
  record_id  BigInt?
  action     AuditAction?
  old_value  String?      @db.Text
  new_value  String?      @db.Text
  created_at DateTime     @default(now()) @db.Timestamp(0)

  @@index([user_id], map: "user_id")
}

model approvals {
  id          BigInt          @id @default(autoincrement())
  voucher_id  BigInt?
  approver_id BigInt?
  status      ApprovalStatus?
  remarks     String?         @db.Text
  approved_at DateTime        @default(now()) @db.Timestamp(0)

  @@index([approver_id], map: "approver_id")
  @@index([voucher_id], map: "voucher_id")
}

model workflows {
  id            BigInt       @id @default(autoincrement())
  company_id    BigInt?
  voucher_type  VoucherType?
  min_amount    Decimal?     @db.Decimal(15, 2)
  approver_role String?      @db.VarChar(100)
  level         Int?

  @@index([company_id], map: "company_id")
}

model recurring_entries {
  id                  BigInt           @id @default(autoincrement())
  company_id          BigInt?
  voucher_template_id BigInt?
  frequency           Frequency?
  next_run            DateTime?        @db.Date
  end_date            DateTime?        @db.Date
  status              RecurringStatus?

  @@index([company_id], map: "company_id")
  @@index([voucher_template_id], map: "voucher_template_id")
}

model notifications {
  id         BigInt            @id @default(autoincrement())
  company_id BigInt?
  user_id    BigInt?
  message    String?           @db.Text
  type       NotificationType?
  is_read    Boolean?
  created_at DateTime          @default(now()) @db.Timestamp(0)

  @@index([company_id], map: "company_id")
  @@index([user_id], map: "user_id")
}

model plans {
  id               BigInt        @id @default(autoincrement())
  plan_name        String?       @db.VarChar(50)
  currency         String?       @db.VarChar(3)
  base_price       Decimal?      @db.Decimal(10, 2)
  total_price      Decimal?      @db.Decimal(10, 2)
  invoice_limit    Int?
  user_limit       Int?
  storage_capacity String?       @db.VarChar(20)
  billing_cycle    BillingCycle?
  status           PlanStatus?
  created_at       DateTime      @default(now()) @db.Timestamp(0)
}

model requested_plans {
  id           BigInt         @id @default(autoincrement())
  company_id   BigInt?
  plan_id      BigInt?
  billing      BillingCycle?
  request_date DateTime?      @db.Date
  status       RequestStatus?
  created_at   DateTime       @default(now()) @db.Timestamp(0)

  @@index([company_id], map: "company_id")
  @@index([plan_id], map: "plan_id")
}

model payment_gateway_settings {
  id              BigInt         @id @default(autoincrement())
  company_id      BigInt?
  gateway_name    String?        @db.VarChar(50)
  status          GatewayStatus?
  api_key         String?        @db.VarChar(255)
  publishable_key String?        @db.VarChar(255)
  secret_key      String?        @db.VarChar(255)
  webhook_url     String?        @db.VarChar(255)
  environment     Environment?
  updated_at      DateTime       @default(now()) @db.Timestamp(0)

  @@index([company_id], map: "company_id")
}

model trial_balance {
  id                BigInt       @id @default(autoincrement())
  company_id        BigInt?
  financial_year_id BigInt?
  account_id        BigInt?
  total_debit       Decimal?     @db.Decimal(15, 2)
  total_credit      Decimal?     @db.Decimal(15, 2)
  closing_balance   Decimal?     @db.Decimal(15, 2)
  balance_type      BalanceType?
  generated_at      DateTime     @default(now()) @db.Timestamp(0)

  @@index([account_id], map: "account_id")
  @@index([company_id], map: "company_id")
  @@index([financial_year_id], map: "financial_year_id")
}

model profit_loss {
  id                BigInt   @id @default(autoincrement())
  company_id        BigInt?
  financial_year_id BigInt?
  period            String?  @db.VarChar(50)
  total_income      Decimal? @db.Decimal(15, 2)
  total_expense     Decimal? @db.Decimal(15, 2)
  net_result        Decimal? @db.Decimal(15, 2)
  generated_at      DateTime @default(now()) @db.Timestamp(0)

  @@index([company_id], map: "company_id")
  @@index([financial_year_id], map: "financial_year_id")
}

model balance_sheet {
  id                BigInt    @id @default(autoincrement())
  company_id        BigInt?
  financial_year_id BigInt?
  as_on_date        DateTime? @db.Date
  total_assets      Decimal?  @db.Decimal(15, 2)
  total_liabilities Decimal?  @db.Decimal(15, 2)
  total_equity      Decimal?  @db.Decimal(15, 2)
  generated_at      DateTime  @default(now()) @db.Timestamp(0)

  @@index([company_id], map: "company_id")
  @@index([financial_year_id], map: "financial_year_id")
}

model cash_flow {
  id                BigInt   @id @default(autoincrement())
  company_id        BigInt?
  financial_year_id BigInt?
  period            String?  @db.VarChar(50)
  cash_in           Decimal? @db.Decimal(15, 2)
  cash_out          Decimal? @db.Decimal(15, 2)
  net_cash_flow     Decimal? @db.Decimal(15, 2)
  generated_at      DateTime @default(now()) @db.Timestamp(0)

  @@index([company_id], map: "company_id")
  @@index([financial_year_id], map: "financial_year_id")
}

model customer_ledger {
  id                BigInt                    @id @default(autoincrement())
  customer_id       BigInt
  date              DateTime                  @db.Date
  particulars       String                    @db.VarChar(255)
  vch_no            String?                   @db.VarChar(30)
  manual_voucher_no String?                   @db.VarChar(30)
  vch_type          customer_ledger_vch_type?
  debit             Decimal?                  @default(0.00) @db.Decimal(15, 2)
  credit            Decimal?                  @default(0.00) @db.Decimal(15, 2)
  running_balance   Decimal                   @db.Decimal(15, 2)
  created_at        DateTime                  @default(now()) @db.Timestamp(0)

  @@index([customer_id, date], map: "idx_ledger_customer_date")
  @@index([customer_id, vch_type, date], map: "idx_ledger_type_date")
  @@index([vch_no], map: "idx_ledger_vchno")
}

model vendor_ledger {
  id              BigInt         @id @default(autoincrement())
  vendor_id       BigInt?
  voucher_id      BigInt?
  date            DateTime?      @db.Date
  particulars     String?        @db.VarChar(255)
  vch_no          String?        @db.VarChar(50)
  vch_type        VendorVchType?
  debit           Decimal?       @db.Decimal(15, 2)
  credit          Decimal?       @db.Decimal(15, 2)
  running_balance Decimal?       @db.Decimal(15, 2)
  created_at      DateTime       @default(now()) @db.Timestamp(0)

  @@index([vendor_id], map: "vendor_id")
  @@index([voucher_id], map: "voucher_id")
}

model dashboard_summary {
  id                BigInt   @id @default(autoincrement())
  company_id        BigInt?
  total_sales       Decimal? @db.Decimal(15, 2)
  total_purchases   Decimal? @db.Decimal(15, 2)
  total_receivables Decimal? @db.Decimal(15, 2)
  total_payables    Decimal? @db.Decimal(15, 2)
  total_expenses    Decimal? @db.Decimal(15, 2)
  top_customer      String?  @db.VarChar(255)
  low_stock_items   Int?
  created_at        DateTime @default(now()) @db.Timestamp(0)

  @@index([company_id], map: "company_id")
}

model add_transactions {
  id               BigInt                         @id @default(autoincrement())
  company_id       BigInt?
  transaction_date DateTime?                      @db.Date
  transaction_id   String?                        @unique(map: "transaction_id") @db.VarChar(50)
  balance_type     add_transactions_balance_type?
  voucher_type     add_transactions_voucher_type?
  voucher_no       String?                        @db.VarChar(50)
  amount           Decimal?                       @db.Decimal(15, 2)
  from_account_id  BigInt?
  to_account_id    BigInt?
  customer_id      BigInt?
  account_type     add_transactions_account_type?
  note             String?                        @db.Text
  created_at       DateTime                       @default(now()) @db.Timestamp(0)

  @@index([company_id], map: "company_id")
  @@index([customer_id], map: "customer_id")
  @@index([from_account_id], map: "from_account_id")
  @@index([to_account_id], map: "to_account_id")
}

model add_units {
  id              BigInt   @id @default(autoincrement())
  unit_of_measure String?  @db.VarChar(100)
  created_at      DateTime @default(now()) @db.Timestamp(0)
}

model bank_details {
  id             Int      @id @default(autoincrement())
  bank_name      String   @db.VarChar(255)
  account_no     String   @unique(map: "account_no") @db.VarChar(100)
  account_holder String   @db.VarChar(255)
  ifsc_code      String   @db.VarChar(50)
  notes          String?  @db.Text
  created_at     DateTime @default(now()) @db.Timestamp(0)
  updated_at     DateTime @default(now()) @db.Timestamp(0)
}

model categories {
  id            BigInt   @id @default(autoincrement())
  company_id    BigInt?
  category_name String?  @db.VarChar(100)
  parent_id     BigInt?
  description   String?  @db.Text
  created_at    DateTime @default(now()) @db.Timestamp(0)

  @@index([company_id], map: "company_id")
  @@index([parent_id], map: "parent_id")
}

model company_settings {
  id           BigInt   @id @default(autoincrement())
  company_name String?  @db.VarChar(255)
  address      String?  @db.Text
  contact      String?  @db.VarChar(50)
  gstin        String?  @db.VarChar(20)
  created_at   DateTime @default(now()) @db.Timestamp(0)
}

model contra_vouchers {
  id                BigInt                  @id @default(autoincrement())
  company_id        BigInt?
  auto_voucher_no   String?                 @db.VarChar(50)
  manual_voucher_no String?                 @db.VarChar(50)
  voucher_date      DateTime?               @db.Date
  account_from_id   BigInt?
  account_to_id     BigInt?
  amount            Decimal?                @db.Decimal(15, 2)
  upload_document   String?                 @db.VarChar(255)
  narration         String?                 @db.Text
  status            contra_vouchers_status? @default(Draft)
  created_by        BigInt?
  created_at        DateTime                @default(now()) @db.Timestamp(0)

  @@index([account_from_id], map: "account_from_id")
  @@index([account_to_id], map: "account_to_id")
  @@index([company_id], map: "company_id")
  @@index([created_by], map: "created_by")
}

model customer_balance_confirms {
  id                   BigInt                                  @id @default(autoincrement())
  customer_id          BigInt
  as_of_date           DateTime                                @db.Date
  opening_balance      Decimal?                                @default(0.00) @db.Decimal(15, 2)
  total_debit          Decimal?                                @default(0.00) @db.Decimal(15, 2)
  total_credit         Decimal?                                @default(0.00) @db.Decimal(15, 2)
  current_balance      Decimal                                 @db.Decimal(15, 2)
  balance_type         customer_balance_confirms_balance_type?
  letter_pdf_path      String?                                 @db.VarChar(500)
  status               customer_balance_confirms_status?       @default(draft)
  confirmed_at         DateTime                                @default(now()) @db.Timestamp(0)
  confirmed_by_user_id BigInt?
  created_at           DateTime                                @default(dbgenerated("(_utf8mb4\\'0000-00-00 00:00:00\\')")) @db.Timestamp(0)

  @@index([customer_id, as_of_date], map: "idx_confirm_customer_date")
}

model customer_ledger_items {
  id               BigInt   @id @default(autoincrement())
  ledger_id        BigInt
  item_name        String   @db.VarChar(255)
  qty              Decimal  @db.Decimal(10, 3)
  rate             Decimal  @db.Decimal(10, 2)
  discount_percent Decimal? @default(0.00) @db.Decimal(5, 2)
  tax_percent      Decimal? @default(0.00) @db.Decimal(5, 2)
  tax_amount       Decimal? @default(0.00) @db.Decimal(10, 2)
  value            Decimal  @db.Decimal(10, 2)
  description      String?  @db.Text

  @@index([ledger_id], map: "idx_ledger_item")
}

model customers {
  id                  BigInt                  @id @default(autoincrement())
  voucher_no          Int                     @unique(map: "voucher_no")
  name_english        String                  @db.VarChar(255)
  name_arabic         String?                 @db.VarChar(255)
  company_name        String?                 @db.VarChar(255)
  google_location     String?                 @db.VarChar(500)
  id_card_image       String?                 @db.VarChar(255)
  any_file            String?                 @db.VarChar(255)
  account_type        customers_account_type? @default(Sundry_Debtors)
  balance_type        customers_balance_type? @default(Debit)
  account_name        String?                 @default("Accounts Receivable") @db.VarChar(100)
  opening_balance     Decimal?                @default(0.00) @db.Decimal(15, 2)
  creation_date       DateTime?               @db.Date
  bank_account_number String?                 @db.VarChar(50)
  bank_ifsc           String?                 @db.VarChar(20)
  bank_name_branch    String?                 @db.VarChar(255)
  country             String?                 @db.VarChar(100)
  state               String?                 @db.VarChar(100)
  pincode             String?                 @db.VarChar(10)
  address             String?                 @db.Text
  state_code          String?                 @db.VarChar(10)
  shipping_address    String?                 @db.Text
  phone               String?                 @db.VarChar(20)
  email               String?                 @db.VarChar(255)
  gstin               String?                 @db.VarChar(20)
  credit_period_days  Int?                    @default(0)
  is_active           Boolean?                @default(true)
  created_at          DateTime                @default(now()) @db.Timestamp(0)
  updated_at          DateTime?               @default(now()) @updatedAt @db.Timestamp(0)

  @@index([email], map: "idx_cust_email")
  @@index([name_english], map: "idx_cust_name")
  @@index([phone], map: "idx_cust_phone")
}

model delivery_challan_items {
  id            BigInt   @id @default(autoincrement())
  challan_id    BigInt?
  item_id       BigInt?
  delivered_qty Decimal? @db.Decimal(15, 3)
  rate          Decimal? @db.Decimal(15, 2)
  tax_percent   Decimal? @db.Decimal(5, 2)
  discount      Decimal? @db.Decimal(15, 2)
  amount        Decimal? @db.Decimal(15, 2)

  @@index([challan_id], map: "challan_id")
  @@index([item_id], map: "item_id")
}

model delivery_challans {
  id            BigInt                    @id @default(autoincrement())
  company_id    BigInt?
  order_id      BigInt?
  challan_no    String?                   @db.VarChar(50)
  manual_ref_no String?                   @db.VarChar(50)
  challan_date  DateTime?                 @db.Date
  driver_name   String?                   @db.VarChar(100)
  driver_phone  String?                   @db.VarChar(20)
  vehicle_no    String?                   @db.VarChar(50)
  total_amount  Decimal?                  @db.Decimal(15, 2)
  terms         String?                   @db.Text
  status        delivery_challans_status?
  created_by    BigInt?
  created_at    DateTime                  @default(now()) @db.Timestamp(0)

  @@index([company_id], map: "company_id")
  @@index([created_by], map: "created_by")
  @@index([order_id], map: "order_id")
}

model eway_bill_items {
  id            BigInt   @id @default(autoincrement())
  ewb_id        BigInt
  product_id    BigInt?
  product_name  String   @db.VarChar(255)
  hsn_code      String?  @db.VarChar(20)
  qty           Decimal  @db.Decimal(12, 3)
  uqc           String?  @db.VarChar(20)
  taxable_value Decimal  @db.Decimal(15, 2)
  cgst_percent  Decimal? @default(0.00) @db.Decimal(5, 2)
  sgst_percent  Decimal? @default(0.00) @db.Decimal(5, 2)
  igst_percent  Decimal? @default(0.00) @db.Decimal(5, 2)
  cgst_amount   Decimal? @default(0.00) @db.Decimal(15, 2)
  sgst_amount   Decimal? @default(0.00) @db.Decimal(15, 2)
  igst_amount   Decimal? @default(0.00) @db.Decimal(15, 2)
  total_value   Decimal? @db.Decimal(15, 2)
